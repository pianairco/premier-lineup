{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "premier-lineup-ui": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "project/premier-lineup-ui",
      "sourceRoot": "project/premier-lineup-ui/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "../premier-lineup-backend/src/main/resources/premier-lineup-ui",
            "index": "project/premier-lineup-ui/src/index.html",
            "main": "project/premier-lineup-ui/src/main.ts",
            "polyfills": "project/premier-lineup-ui/src/polyfills.ts",
            "tsConfig": "project/premier-lineup-ui/tsconfig.app.json",
            "aot": true,
            "assets": [
              "project/premier-lineup-ui/src/favicon.ico",
              "project/premier-lineup-ui/src/assets"
            ],
            "styles": [
              "node_modules/bootstrap/dist/css/bootstrap.rtl.min.css",
              "project/premier-lineup-ui/src/styles.css"
            ],
            "scripts": [
              "./node_modules/bootstrap/dist/js/bootstrap.bundle.min.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "project/premier-lineup-ui/src/environments/environment.ts",
                  "with": "project/premier-lineup-ui/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "premier-lineup-ui:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "premier-lineup-ui:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "premier-lineup-ui:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "project/premier-lineup-ui/src/test.ts",
            "polyfills": "project/premier-lineup-ui/src/polyfills.ts",
            "tsConfig": "project/premier-lineup-ui/tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "project/premier-lineup-ui/src/favicon.ico",
              "project/premier-lineup-ui/src/assets"
            ],
            "styles": [
              "project/premier-lineup-ui/src/styles.css"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "project/premier-lineup-ui/tsconfig.app.json",
              "project/premier-lineup-ui/tsconfig.spec.json",
              "e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "premier-lineup-ui:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "premier-lineup-ui:serve:production"
            }
          }
        }
      }
    }
  },
  "defaultProject": "premier-lineup-ui"
}
